{
  "name": "@tonaljs/tonal",
  "version": "4.6.8",
  "description": "@tonaljs music theory full library",
  "keywords": [
    "music",
    "theory",
    "music-theory"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist",
    "browser"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@tonaljs/abc-notation": "^4.6.8",
    "@tonaljs/array": "^4.6.8",
    "@tonaljs/chord": "^4.6.8",
    "@tonaljs/chord-type": "^4.6.8",
    "@tonaljs/collection": "^4.6.8",
    "@tonaljs/core": "^4.6.8",
    "@tonaljs/duration-value": "^4.6.8",
    "@tonaljs/interval": "^4.6.8",
    "@tonaljs/key": "^4.6.8",
    "@tonaljs/midi": "^4.6.8",
    "@tonaljs/mode": "^4.6.8",
    "@tonaljs/note": "^4.6.8",
    "@tonaljs/pcset": "^4.6.8",
    "@tonaljs/progression": "^4.6.8",
    "@tonaljs/range": "^4.6.8",
    "@tonaljs/roman-numeral": "^4.6.8",
    "@tonaljs/scale": "^4.6.8",
    "@tonaljs/scale-type": "^4.6.8",
    "@tonaljs/time-signature": "^4.6.8"
  },
  "author": "danigb@gmail.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run build:node && npm run build:browser",
    "build:node": "tsup index.ts --sourcemap --dts --format esm,cjs",
    "build:browser": "esbuild index.ts --bundle --minify --sourcemap --outfile=browser/tonal.min.js",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  }
}
