{
  "name": "tonal",
  "version": "5.2.1",
  "description": "tonaljs music theory library",
  "keywords": [
    "music",
    "theory",
    "music-theory"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "README.md",
    "dist",
    "browser"
  ],
  "types": "dist/index.d.ts",
  "dependencies": {
    "@tonaljs/abc-notation": "4.8.2",
    "@tonaljs/array": "4.8.2",
    "@tonaljs/chord-type": "5.0.4",
    "@tonaljs/chord": "5.0.3",
    "@tonaljs/collection": "4.8.1",
    "@tonaljs/core": "4.10.4",
    "@tonaljs/duration-value": "4.8.1",
    "@tonaljs/interval": "4.8.2",
    "@tonaljs/key": "4.9.3",
    "@tonaljs/midi": "4.9.2",
    "@tonaljs/mode": "4.8.3",
    "@tonaljs/note": "4.10.2",
    "@tonaljs/pcset": "4.9.1",
    "@tonaljs/progression": "4.8.3",
    "@tonaljs/range": "4.8.3",
    "@tonaljs/roman-numeral": "4.8.2",
    "@tonaljs/scale-type": "4.8.4",
    "@tonaljs/scale": "4.12.4",
    "@tonaljs/time-signature": "4.8.1",
    "@tonaljs/voice-leading": "5.0.2",
    "@tonaljs/voicing-dictionary": "5.0.2",
    "@tonaljs/voicing": "5.0.2"
  },
  "author": "danigb@gmail.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tonaljs/tonal.git",
    "directory": "packages/tonal"
  },
  "scripts": {
    "build": "npm run build:node && npm run build:browser",
    "build:node": "tsup index.ts --sourcemap --dts --format esm,cjs",
    "build:browser": "esbuild index.ts --format=iife --global-name=Tonal --bundle --minify --sourcemap --outfile=browser/tonal.min.js",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  }
}
